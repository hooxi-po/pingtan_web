<svg width="400" height="300" viewBox="0 0 400 300" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="morningSky" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#FFE4B5;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#87CEEB;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="calmSea" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#20B2AA;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#008B8B;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Morning Sky -->
  <rect width="400" height="180" fill="url(#morningSky)"/>
  
  <!-- Sea -->
  <rect y="180" width="400" height="120" fill="url(#calmSea)"/>
  
  <!-- Fishing Village Houses -->
  <!-- House 1 -->
  <rect x="60" y="140" width="80" height="80" fill="#DEB887" stroke="#8B7355" stroke-width="2"/>
  <polygon points="60,140 100,110 140,140" fill="#CD853F"/>
  <rect x="80" y="170" width="20" height="30" fill="#654321"/>
  <rect x="110" y="155" width="15" height="15" fill="#87CEEB"/>
  
  <!-- House 2 -->
  <rect x="160" y="130" width="90" height="90" fill="#F5DEB3" stroke="#D2B48C" stroke-width="2"/>
  <polygon points="160,130 205,100 250,130" fill="#DAA520"/>
  <rect x="190" y="170" width="25" height="35" fill="#8B4513"/>
  <rect x="170" y="145" width="18" height="18" fill="#87CEEB"/>
  <rect x="220" y="145" width="18" height="18" fill="#87CEEB"/>
  
  <!-- House 3 -->
  <rect x="270" y="145" width="70" height="75" fill="#DDBEA9" stroke="#A0522D" stroke-width="2"/>
  <polygon points="270,145 305,120 340,145" fill="#B8860B"/>
  <rect x="290" y="175" width="20" height="30" fill="#654321"/>
  <rect x="315" y="160" width="15" height="15" fill="#87CEEB"/>
  
  <!-- Fishing Boats -->
  <ellipse cx="80" cy="250" rx="25" ry="8" fill="#8B4513"/>
  <rect x="75" y="245" width="10" height="15" fill="#654321"/>
  <line x1="80" y1="245" x2="80" y2="235" stroke="#654321" stroke-width="2"/>
  
  <ellipse cx="180" cy="260" rx="30" ry="10" fill="#A0522D"/>
  <rect x="175" y="255" width="10" height="18" fill="#654321"/>
  <line x1="180" y1="255" x2="180" y2="240" stroke="#654321" stroke-width="2"/>
  
  <ellipse cx="320" cy="255" rx="20" ry="6" fill="#8B4513"/>
  <rect x="318" y="252" width="8" height="12" fill="#654321"/>
  
  <!-- Fishing Nets -->
  <path d="M20,200 Q40,190 60,200 Q80,210 100,200" stroke="#696969" stroke-width="2" fill="none"/>
  <path d="M25,205 Q45,195 65,205 Q85,215 105,205" stroke="#696969" stroke-width="1" fill="none"/>
  
  <!-- Seagulls -->
  <path d="M120,80 Q125,75 130,80" stroke="#666" stroke-width="2" fill="none"/>
  <path d="M200,70 Q205,65 210,70" stroke="#666" stroke-width="2" fill="none"/>
  <path d="M300,90 Q305,85 310,90" stroke="#666" stroke-width="2" fill="none"/>
  
  <!-- Dock/Pier -->
  <rect x="0" y="220" width="120" height="8" fill="#8B4513"/>
  <rect x="10" y="215" width="4" height="20" fill="#654321"/>
  <rect x="30" y="215" width="4" height="20" fill="#654321"/>
  <rect x="50" y="215" width="4" height="20" fill="#654321"/>
  <rect x="70" y="215" width="4" height="20" fill="#654321"/>
  <rect x="90" y="215" width="4" height="20" fill="#654321"/>
  <rect x="110" y="215" width="4" height="20" fill="#654321"/>
  
  <!-- Fishing Equipment -->
  <circle cx="40" cy="210" r="3" fill="#FF6347"/>
  <circle cx="60" cy="212" r="2" fill="#FF6347"/>
  <circle cx="80" cy="208" r="3" fill="#FF6347"/>
  
  <!-- Sun -->
  <circle cx="350" cy="50" r="20" fill="#FFD700"/>
  <line x1="330" y1="30" x2="335" y2="35" stroke="#FFD700" stroke-width="2"/>
  <line x1="365" y1="35" x2="370" y2="30" stroke="#FFD700" stroke-width="2"/>
  <line x1="330" y1="70" x2="335" y2="65" stroke="#FFD700" stroke-width="2"/>
  <line x1="365" y1="65" x2="370" y2="70" stroke="#FFD700" stroke-width="2"/>
  
  <!-- Clouds -->
  <circle cx="100" cy="40" r="15" fill="white" opacity="0.8"/>
  <circle cx="115" cy="35" r="12" fill="white" opacity="0.8"/>
  <circle cx="250" cy="60" r="18" fill="white" opacity="0.8"/>
  <circle cx="265" cy="55" r="14" fill="white" opacity="0.8"/>
</svg>