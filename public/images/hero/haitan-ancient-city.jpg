<svg width="1920" height="1080" viewBox="0 0 1920 1080" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="skyGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FFE4B5;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#DDA0DD;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8B4513;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="buildingGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#8B4513;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#A0522D;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- 天空背景 -->
  <rect width="1920" height="600" fill="url(#skyGradient)"/>
  
  <!-- 古建筑群 -->
  <rect x="200" y="400" width="300" height="400" fill="url(#buildingGradient)" rx="10"/>
  <rect x="600" y="350" width="250" height="450" fill="url(#buildingGradient)" rx="10"/>
  <rect x="950" y="380" width="280" height="420" fill="url(#buildingGradient)" rx="10"/>
  <rect x="1350" y="320" width="320" height="480" fill="url(#buildingGradient)" rx="10"/>
  
  <!-- 屋顶 -->
  <polygon points="200,400 350,300 500,400" fill="#CD853F"/>
  <polygon points="600,350 725,250 850,350" fill="#CD853F"/>
  <polygon points="950,380 1090,280 1230,380" fill="#CD853F"/>
  <polygon points="1350,320 1510,220 1670,320" fill="#CD853F"/>
  
  <!-- 窗户装饰 -->
  <rect x="250" y="500" width="40" height="60" fill="#654321" rx="5"/>
  <rect x="350" y="500" width="40" height="60" fill="#654321" rx="5"/>
  <rect x="650" y="450" width="40" height="60" fill="#654321" rx="5"/>
  <rect x="750" y="450" width="40" height="60" fill="#654321" rx="5"/>
  
  <!-- 地面 -->
  <rect y="800" width="1920" height="280" fill="#8B7355"/>
  
  <!-- 装饰性云朵 -->
  <ellipse cx="400" cy="150" rx="80" ry="40" fill="white" opacity="0.8"/>
  <ellipse cx="1200" cy="100" rx="100" ry="50" fill="white" opacity="0.7"/>
  

</svg>