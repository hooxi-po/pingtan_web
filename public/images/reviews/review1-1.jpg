<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="beachGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="70%" style="stop-color:#4682B4;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#F4E4BC;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="300" fill="url(#beachGradient)"/>
  
  <!-- Beach -->
  <rect y="200" width="400" height="100" fill="#F4E4BC"/>
  
  <!-- Footprints -->
  <ellipse cx="100" cy="230" rx="8" ry="12" fill="#D2B48C"/>
  <ellipse cx="110" cy="240" rx="8" ry="12" fill="#D2B48C"/>
  <ellipse cx="120" cy="250" rx="8" ry="12" fill="#D2B48C"/>
  
  <!-- Seashells -->
  <ellipse cx="200" cy="220" rx="6" ry="4" fill="#FFB6C1"/>
  <ellipse cx="250" cy="240" rx="5" ry="3" fill="#FFA07A"/>
  <ellipse cx="300" cy="230" rx="7" ry="5" fill="#F0E68C"/>
  
  <!-- Waves -->
  <path d="M0,180 Q100,160 200,180 T400,180" stroke="white" stroke-width="3" fill="none" opacity="0.8"/>
  <path d="M0,190 Q80,170 160,190 T320,190 T400,190" stroke="white" stroke-width="2" fill="none" opacity="0.6"/>
  
  <!-- Text -->
  <text x="200" y="280" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#8B4513">游客拍摄 - 海滩漫步</text>
</svg>